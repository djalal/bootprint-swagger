{{!--
    Renders the definition-section of the HTML-page.
    @param {Definition[]} definitions a list of JSON-subschemas.
    @api public
--}}

{{#if definitions}}
    <h2>Model definitions</h2>

    {{#each definitions}}
        {{>swagger/model model=. title=@key anchor="/definitions" }}
    {{/each}}
{{/if}}
